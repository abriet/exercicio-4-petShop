<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="texto_titulo">
        <item name="android:textSize">@dimen/font_titulo</item>
        <item name="android:textColor">@color/text_titulo</item>
    </style>

    <style name="texto_resultado">
        <item name="android:textSize">@dimen/font_resultado</item>
        <item name="android:textColor">@color/text_titulo</item>
        <item name="android:textAlignment">center</item>
    </style>


    <dimen name="font_resultado">60dp</dimen>
</resources>
